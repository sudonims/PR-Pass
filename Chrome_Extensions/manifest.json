{
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "password generator"
  },
  "web_accessible_resources": ["cipher.json"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "logo.png"
  },
  "permissions": ["storage", "notifications", "contextMenus"],
  "name": "PR-Pass",
  "version": "1.0.3",
  "description": "Easy to create passwords",
  "author": "Ekansh Gupta - Nimish Shah",
  "manifest_version": 2
}
